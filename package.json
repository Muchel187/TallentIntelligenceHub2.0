{
  "name": "noba-experts",
  "version": "0.1.0",
  "description": "NOBA EXPERTS - Scientific Big Five Personality Assessment Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "ai:verify": "node scripts/ai/verify-environment.cjs",
    "ai:status": "node scripts/ai/project-status.cjs",
    "ai:next": "node scripts/ai/suggest-next.cjs",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:smoke": "vitest run test/smoke",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node database/seeds/seed.js",
    "db:reset": "prisma migrate reset",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next out dist coverage .turbo node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.16.3",
    "@react-email/components": "^0.5.6",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "next": "^14.2.33",
    "next-auth": "^5.0.0-beta.29",
    "puppeteer": "^24.23.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.1.2",
    "stripe": "^19.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.19",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.16.3",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "personality-test",
    "big-five",
    "hr-analytics",
    "ai-coaching",
    "saas",
    "typescript",
    "next.js"
  ]
}
